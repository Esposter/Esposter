{
  "name": "esposter",
  "description": "A nice and casual place for posting random things.",
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://github.com/Esposter/Esposter#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esposter/Esposter.git"
  },
  "bugs": {
    "url": "https://github.com/Esposter/Esposter/issues"
  },
  "scripts": {
    "build": "lerna run build",
    "start": "lerna run start --scope=@esposter/app",
    "test": "lerna run test",
    "typecheck": "lerna run typecheck",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix",
    "release": "pnpm run lint:fix --no-private && pnpm run build --no-private && lerna publish --yes",
    "refresh:lockfile": "pnpm pwsh scripts/refreshLockfile.ps1"
  },
  "dependencies": {
    "data-urls": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/data-urls": "^3.0.4",
    "@types/xml2js": "^0.4.14",
    "ctix": "^2.4.4",
    "eslint": "^9.3.0",
    "lerna": "^8.1.3",
    "rollup": "^4.18.0",
    "typescript": "5.5.0-beta"
  },
  "engines": {
    "node": "^22.2.0"
  },
  "packageManager": "pnpm@9.1.2+sha512.127dc83b9ea10c32be65d22a8efb4a65fb952e8fefbdfded39bdc3c97efc32d31b48b00420df2c1187ace28c921c902f0cb5a134a4d032b8b5295cbfa2c681e2"
}
