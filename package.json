{
  "name": "esposter",
  "description": "A nice and casual place for posting random things.",
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://github.com/Esposter/Esposter#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esposter/Esposter.git"
  },
  "bugs": {
    "url": "https://github.com/Esposter/Esposter/issues"
  },
  "scripts": {
    "build": "lerna run build",
    "start": "lerna run start --scope=@esposter/app",
    "test": "lerna run test",
    "typecheck": "lerna run typecheck",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix",
    "release": "pnpm run lint:fix --no-private && pnpm run build --no-private && lerna publish --yes",
    "refresh:lockfile": "pnpm pwsh scripts/refreshLockfile.ps1"
  },
  "dependencies": {
    "data-urls": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/data-urls": "^3.0.4",
    "@types/xml2js": "^0.4.14",
    "ctix": "^2.4.4",
    "eslint": "^9.3.0",
    "lerna": "^8.1.3",
    "rollup": "^4.18.0",
    "typescript": "^5.5.0-beta"
  },
  "pnpm": {
    "overrides": {
      "typescript": "$typescript",
      "@esposter/app>typescript": "^5.4.5"
    }
  },
  "engines": {
    "node": "^22.2.0"
  },
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
}
