{
  "name": "esposter",
  "description": "A nice and casual place for posting random things.",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 nuxt build",
    "dev": "nuxt dev",
    "start": "node .output/server/index.mjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "cleanup": "nuxt cleanup",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ext .js,.ts,.vue --cache --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.ts,.vue --cache --ignore-path .gitignore --fix .",
    "db:run": "tsx --tsconfig ./.nuxt/tsconfig.json node_modules/drizzle-kit/bin.cjs",
    "db:gen": "pnpm run db:run generate:pg",
    "db:studio": "pnpm run db:run studio",
    "gen-env": "gen-env-types .env -o env.d.ts -e ."
  },
  "scriptsComments": {
    "build": "https://rollupjs.org/troubleshooting/#error-javascript-heap-out-of-memory",
