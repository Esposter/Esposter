{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "oxc.oxc-vscode",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "files.associations": {
    ".env": "env",
    ".env.example": "env",
    ".tsx": "xml"
  },
  "search.followSymlinks": false,
  "symlink-follow.autoFollow": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.tsserver.maxTsServerMemory": 8000,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascript][typescript][vue]": {
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": ["source.organizeImports", "source.fixAll.oxc", "source.fixAll.eslint"]
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[scss]": {
    "editor.defaultFormatter": "sibiraj-s.vscode-scss-formatter"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "[xml]": {
    "editor.formatOnSave": false
  },
  "oxc.enable": true,
  "azureFunctions.deploySubpath": "packages\\azure-functions",
  "azureFunctions.preDeployTask": "npm prune (functions)",
  "azureFunctions.postDeployTask": "npm install (functions)",
  "azureFunctions.projectLanguage": "JavaScript",
  "azureFunctions.projectRuntime": "~4",
  "debug.internalConsoleOptions": "neverOpen"
}
