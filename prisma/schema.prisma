datasource db {
  // provider = "postgres"
  // url      = env("DATABASE_URL")
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Room {
  id        String   @id @default(uuid())
  name      String
  avatar    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
