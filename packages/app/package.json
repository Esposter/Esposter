  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 nuxt build",
    "dev": "nuxt dev",
    "start": "node .output/server/index.mjs",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vitest",
    "coverage": "vitest run --coverage",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "cross-env NODE_OPTIONS=--max-old-space-size=8192 nuxt typecheck",
    "cleanup": "nuxt cleanup",
    "postinstall": "nuxt prepare",
    "lint": "cross-env TIMING=1 eslint --config eslint.light.config.js .",
    "lint:fix": "cross-env TIMING=1 eslint --config eslint.light.config.js --fix .",
    "lint:all": "cross-env TIMING=1 eslint .",
    "lint:all:fix": "cross-env TIMING=1 eslint --fix .",
    "db:run": "tsx node_modules/drizzle-kit/bin.cjs",
    "db:gen": "pnpm db:run generate",
    "db:studio": "pnpm db:run studio",
    "db:up": "pnpm db:run up",
    "gen-env": "pnpx gen-env-types .env -o env.d.ts -e .",
    "phaser:gen": "tsx scripts/phaser/index.ts",
    "tiled:gen": "tsx scripts/tiled/index.ts",
    "generate-pwa-assets": "pnpx pwa-asset-generator https://pshpstespauea001.blob.core.windows.net/esposter-assets/logo.png public/pwa -o false -f -i public/pwa/index.html -m public/pwa/manifest.json",
    "pwsh": "pwsh -ExecutionPolicy Unrestricted"
  },
  "scriptsComments": {
    "build": "https://rollupjs.org/troubleshooting/#error-javascript-heap-out-of-memory",
    "db:run": "https://github.com/drizzle-team/drizzle-orm/issues/1228"
  },
