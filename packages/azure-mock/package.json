    "lint": "oxlint && eslint .",
    "lint:fix": "oxlint --fix && eslint --fix .",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@esposter/db": "workspace:*",
    "@esposter/db-schema": "workspace:*",
    "@esposter/shared": "workspace:*",
    "drizzle-orm": "catalog:",
    "drizzle-zod": "catalog:",
    "fast-deep-equal": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@azure/core-http-compat": "catalog:",
    "@azure/core-rest-pipeline": "catalog:",
    "@azure/data-tables": "catalog:",
    "@azure/eventgrid": "catalog:",
    "@azure/storage-blob": "catalog:",
    "@azure/storage-queue": "catalog:"
  }
}
