{
  "concurrency": {
    "dynamicConcurrencyEnabled": true
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "excludedTypes": "Request"
      }
    },
    "fileLoggingMode": "never",
    "logLevel": {
      "Host": "None"
    }
  },
  "version": "2.0"
}
