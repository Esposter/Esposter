{
  "name": "@esposter/xml2js",
  "version": "2.18.2",
  "description": "Simple XML to JavaScript object converter.",
  "keywords": [
    "json",
    "xml"
  ],
  "homepage": "https://github.com/Esposter/Esposter/blob/main/packages/xml2js#readme",
  "bugs": {
    "url": "https://github.com/Esposter/Esposter/issues"
  },
  "license": "Apache-2.0",
  "author": "Jimmy Chen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esposter/Esposter.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm export:gen && rolldown --config rolldown.config.ts",
    "export:gen": "ctix build --config ../configuration/.ctirc-ts",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "lint": "oxlint && eslint .",
    "lint:fix": "oxlint --fix && eslint --fix .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "sax": "catalog:",
    "xmlbuilder2": "catalog:"
  },
  "devDependencies": {
    "@rolldown/plugin-node-polyfills": "catalog:",
    "@types/sax": "catalog:",
    "@types/xml2js": "catalog:"
  }
}
