    "types:gen": "dts-bundle-generator -o dist/index.d.ts src/index.ts"
  },
  "dependencies": {
    "@esposter/shared": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.12",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.3.0",
    "rollup": "^4.17.2",
    "typescript": "^5.4.5"
  },
  "gitHead": "000547393d835453249a06f46fc9e652eea3119e"
}
