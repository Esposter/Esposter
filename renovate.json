{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"]
    }
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "automerge": true,
  "automergeType": "branch"
}
